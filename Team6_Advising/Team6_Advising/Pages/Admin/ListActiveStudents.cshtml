@page
@model Team6_Advising.Pages.Admin.ListActiveStudentsModel
@{
    Layout = "./Shared/_LayoutAdmin";
}
<h1>Active Students</h1>

<table class="table">
    <thead>
        <tr>
            <th>Student ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Password</th>
            <th>GPA</th>
            <th>Faculty</th>
            <th>Email</th>
            <th>Major</th>
            <th>Semester</th>
            <th>Acquired Hours</th>
            <th>Assigned Hours</th>
            <th>Advisor ID</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.activeStudents)
        {
            <tr>
                <td>@item.student_id</td>
                <td>@item.fname</td>
                <td>@item.lname</td>
                <td>@item.password</td>
                <td>@item.gpa</td>
                <td>@item.faculty</td>
                <td>@item.email</td>
                <td>@item.major</td>
                <td>@item.semester</td>
                <td>@item.acquired_hours</td>
                <td>@item.assigned_hours</td>
                <td>@item.advisor_id</td>
            </tr>
        }
    </tbody>
</table>